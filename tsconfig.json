{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6"
      ],
      "target": "es2016",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./build",
      "esModuleInterop": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true
   },
   "exclude": ["node_modules"]
}
